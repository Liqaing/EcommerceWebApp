@model List<AppUser>

<div class="container mt-4">
    <div class="row">
        <div class="col-6">
            <h2>Accounts List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-area="Identity" asp-page="/Account/Register" class="btn btn-primary">
                Create Account
            </a>
        </div>
    </div>

    <table id="tb" class="table table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>Role</th>
                <th>Phone Number</th>
                <th>Email</th>
                <th>Status</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
</div>

@section Scripts {
    <script src="~/js/Account/admin/Index.js"></script>
}